				"effects": {
					"x": 0,
					"y": 20
				},
				"ease": "Power0.easeNone",
				"duration": 500
			}, {
				"effects": {
					"x": 0,
					"y": 0
				},
				"ease": "Power0.easeNone",
				"duration": 500
			}],
			"yoyo": false,
			"name": "Pacman"
